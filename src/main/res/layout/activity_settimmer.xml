<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".settimmerActivity">

    <Button
        android:text="완료"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/alarmset"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="44dp"
        app:layout_constraintTop_toBottomOf="@+id/time1" app:layout_constraintStart_toEndOf="@+id/alarmsetcancle"/>
    <Button
        android:text="취소"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/alarmsetcancle" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginLeft="192dp" android:layout_marginStart="192dp" android:layout_marginTop="44dp"
        app:layout_constraintTop_toBottomOf="@+id/time1"/>


    <TimePicker
        android:id="@+id/time1"
        android:layout_width="381dp"
        android:layout_height="369dp" app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.287" android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:layout_marginRight="8dp" android:layout_marginEnd="8dp">

    </TimePicker>

</android.support.constraint.ConstraintLayout>